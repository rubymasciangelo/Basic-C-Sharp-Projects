            <Joins>
              <Join Type="CROSS JOIN" ColumnOwner="Film" />
              <Join Type="INNER JOIN" ColumnOwner="Director">
                <On>
                  <From ColumnOwner="Film" ColumnName="DirectorID" />
                  <To ColumnOwner="Director" ColumnName="DirectorID" />
