  <DataSets>
    <DataSet Name="MovieDataSet">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT
  Director.FullName
  ,Film.Title
  ,Film.ReleaseDate
  ,Film.RunTimeMinutes
  ,Film.OscarWins
  ,Genre.Genre
FROM
  Film
  INNER JOIN Director
    ON Film.DirectorID = Director.DirectorID
  INNER JOIN Genre
    ON Film.GenreID = Genre.GenreID</CommandText>
